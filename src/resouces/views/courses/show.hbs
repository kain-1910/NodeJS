<div class="mt-4">
    <div class="row">
        {{#with course}}
            <div class="col-3">
                <button class='btn btn-primary'>Hoc Ngay</button>
            </div>
            <div class="col-9">
                <h2>{{this.name}}</h2>
                <iframe width="560" height="315" src="https://www.youtube.com/embed/{{this.videoId}}" frameborder="0"></iframe>
                <p>{{this.decs}}</p>
            </div>
        {{/with}}
    </div>
</div>